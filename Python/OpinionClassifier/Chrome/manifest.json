{
    "name": "AsdTest Grabber",
    "description": "Extract all staff from current web page",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {},
    "action": {
        "default_popup" : "popup.html"
    },
    "permissions": [
        "storage",
        "declarativeContent",
        "activeTab",
        "scripting",
        "notifications",
        "webNavigation",
        "contextMenus"
    ],
    
    "content_scripts": [
        {
          "matches": [ "<all_urls>" ],
          "js": [ "core/reader.js" ]
        }
      ]      
}